@using AlluringDecors.Models;
<div id="ServiceFormType">
    <h2>Create Service Type</h2>
    <hr />
    <div class="form-group">
        <label for="CreateServiceName">Name</label>
        <input type="text" class="form-control" id="CreateServiceName">
    </div>
    <div class="form-group">
        <label for="CreateServiceDescription">Description</label>
        <textarea class="form-control" id="CreateServiceDescription"></textarea>
    </div>
    <button type="button" onclick="createService()" class="btn btn-primary">Create</button>
</div>